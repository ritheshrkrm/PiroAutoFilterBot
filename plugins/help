@Client.on_message(filters.command("series", CMD))
async def check_series(_, message):
    await message.reply_text("âš ï¸â—ï¸ ğ—¦ğ—²ğ—¿ğ—¶ğ—²ğ˜€ ğ—¥ğ—²ğ—¾ğ˜‚ğ—²ğ˜€ğ˜ ğ—™ğ—¼ğ—¿ğ—ºğ—®ğ˜ â—ï¸âš ï¸\nğŸ—£ ğ–²ğ–¾ğ—‹ğ—‚ğ–¾ğ—Œ ğ–­ğ–ºğ—†ğ–¾,ğ–²ğ–¾ğ–ºğ—Œğ—ˆğ—‡ (ğ–¶ğ—ğ—‚ğ–¼ğ— ğ–²ğ–¾ğ–ºğ—Œğ—ˆğ—‡ ğ—’ğ—ˆğ— ğ—ğ–ºğ—‡ğ—) ğŸ§ \n\nğŸ–‡ğ„ğ±ğšğ¦ğ©ğ¥ğ: \nâ€¢ Game Of Thrones S03âœ…\nâ€¢ Sex Education S02 720pâœ… \nâ€¢ Breaking Bad S01E05âœ… \nâ€¢ Prison Break 1080pâœ… \nâ€¢ Witcher S02âœ… \n\nâŒ [ğ——ğ—¼ğ—»ğ˜ ğ—¨ğ˜€ğ—² ğ˜„ğ—¼ğ—¿ğ—±ğ˜€ ğ—Ÿğ—¶ğ—¸ğ—² ğ—¦ğ—²ğ—®ğ˜€ğ—¼ğ—»/ğ—˜ğ—½ğ—¶ğ˜€ğ—¼ğ—±ğ—²/ğ—¦ğ—²ğ—¿ğ—¶ğ—²ğ˜€ ğ—²ğ˜ğ—°] âŒ")

