import time
import random
from pyrogram import Client, filters

CMD = ["/", "."]

@Client.on_message(filters.command("help", CMD))
async def check_help(_, message):
    await message.reply_text("ğ–§ğ—‚ğ— /movie ğ–¥ğ—ˆğ—‹ ğ–¬ğ—ˆğ—ğ—‚ğ–¾ ğ–²ğ–¾ğ–ºğ—‹ğ–¼ğ— ğ–±ğ—ğ—…ğ–¾ğ—Œ ğŸ“ƒ\n\nğ–§ğ—‚ğ— /series ğ–¥ğ—ˆğ—‹ ğ–²ğ–¾ğ—‹ğ—‚ğ–¾ğ—Œ ğ–²ğ–¾ğ–ºğ—‹ğ–¼ğ— ğ–±ğ—ğ—…ğ–¾ğ—Œ ğŸ“ƒ\n\n\n ğ–¥ğ—ˆğ—‹ ğ–¯ğ—‹ğ—ˆğ—‰ğ–¾ğ—‹ ğ–³ğ—ğ—ğ—ˆğ—‹ğ—‚ğ–ºğ—… ğ–µğ—‚ğ–½ğ–¾ğ—ˆğ—Œ ğ–§ğ—‚ğ— /tutorialğŸ¤—")


